<?xml version="1.0" encoding="utf-8"?>
<mCancelarNfseEnvio xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.abrasf.org.br/nfse.xsd">
  <Pedido>
    <InfPedidoCancelamento Id="{idPedidoCancelamento}">
      <IdentificacaoNfse>
        <Numero>{numeroNfse}</Numero>
        <!--número da nota fiscal de serviços-->
        <CpfCnpj>
            <Cnpj>{cpfCnpjPrestador}</Cnpj>
        </CpfCnpj>
        <InscricaoMunicipal>{inscricaoMunicipalPrestador}</InscricaoMunicipal>
        <CodigoMunicipio>{codigoMunicipio}</CodigoMunicipio>
      </IdentificacaoNfse>
        <CodigoCancelamento>1</CodigoCancelamento>
      <MotivoCancelamento>Problema na geração da nota</MotivoCancelamento>
    </InfPedidoCancelamento>
  </Pedido>
</mCancelarNfseEnvio>